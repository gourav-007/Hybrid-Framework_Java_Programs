   public static void main(String[]args) {
    int n=5;
    for(int i=0;i<n;i++){
        int icj = 1;
        for(int j=0;j<=i;j++){
            System.out.print(icj+"\t");
            int icjplus1 = icj * (i-j)/(j+1);
            icj = icjplus1;
        }
        
        System.out.println();
    }
}
