import java.io.*;
import java.util.Arrays;

class JosephProblem {

    private static int josProblem(int n,int k){
            
        if(n==1)
        {
            return 0;
        }
        
        return (josProblem(n-1,k)+k)%n;
    
    }

	public static void main (String[] args) {
	  System.out.println(josProblem(5,3));
	}
}
