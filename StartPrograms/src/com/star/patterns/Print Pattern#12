//Triangular Fibonacci Pattern  
  
  public static void main(String[]args) {
    int n=5;
    int a=0,b=1;
    for(int i=1;i<=n;i++){
        for(int j=0;j<=i;j++){
            int c=a+b;
            a=b;
            b=c;
            
            System.out.print(a+"\t");
        }
    System.out.println();
    }
}
